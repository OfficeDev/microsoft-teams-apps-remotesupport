{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0.0",
  "id": "78f1b2b2-d7d8-4e10-ba68-7ad587d3a56e",
  "packageName": "com.microsoft.teams.remotesupport.experts",
  "developer": {
    "name": "<<Developer name>>",
    "websiteUrl": "<<WebsiteUrl>>",
    "privacyUrl": "<<PrivacyUrl>>",
    "termsOfUseUrl": "<<TermsOfUseUrl>>"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Remote Support-Expert"
  },
  "description": {
    "short": "Acknowledge and work with expert team on user requests.",
    "full": "Manage support request and set up on call experts in team. Search Urgent, assigned and unassigned requests right from expert team."
  },
  "accentColor": "#64A2CC",
  "bots": [
    {
      "botId": "<<Bot id>>",
      "scopes": [
        "team"
      ],
      "commandLists": [
        {
          "scopes": [
            "team"
          ],
          "commands": [
            {
              "title": "Expert list",
              "description": "List of experts"
            }
          ]
        }
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "composeExtensions": [
    {
      "botId": "<<Bot id>>",
      "canUpdateConfiguration": false,
      "commands": [
        {
          "id": "urgentrequests",
          "type": "query",
          "title": "Urgent",
          "description": "Search urgent requests",
          "parameters": [
            {
              "name": "searchText",
              "title": "Search",
              "description": "Search requests"
            }
          ],
          "context": [
            "compose",
            "commandBox"
          ],
          "initialRun": true
        },
        {
          "id": "assignedrequests",
          "type": "query",
          "title": "Assigned",
          "description": "Search requests assigned to an expert",
          "parameters": [
            {
              "name": "searchText",
              "title": "Search",
              "description": "Search requests"
            }
          ],
          "context": [
            "compose",
            "commandBox"
          ],
          "initialRun": true
        },
        {
          "id": "unassignedrequests",
          "type": "query",
          "title": "Unassigned",
          "description": "Search unassigned requests not yet assigned to an expert",
          "parameters": [
            {
              "name": "searchText",
              "title": "Search",
              "description": "Search requests"
            }
          ],
          "context": [
            "compose",
            "commandBox"
          ],
          "initialRun": true
        }
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "token.botframework.com",
    "*.azurewebsites.net"
  ]
}